<?xml version="1.0" encoding="UTF-8"?>
<Server port="8081" shutdown="SHUTDOWN">
	<GlobalNamingResources>
		<!--jndi config -->
	</GlobalNamingResources>
	<Service name="Catalina">
		<Executor name="tomcatThreadPool" namePrefix="HTTP-8888-exec-"
			maxThreads="400" minSpareThreads="10" maxIdleTime="60000" />
		<Connector port="80" protocol="HTTP/1.1" acceptCount="200"
			keepAliveTimeout="5000" useBodyEncodingForURI="true" enableLookups="false"
			compression="on" compressionMinSize="2048"
			compressableMimeType="text/html,application/xhtml+xml,text/xml,text/css,text/javascript"
			connectionTimeout="20000" redirectPort="8443" executor="tomcatThreadPool"
			maxHttpHeaderSize="1024000" URIEncoding="UTF-8" />
		<Connector port="8082" protocol="AJP/1.3" redirectPort="8443"
			URIEncoding="UTF-8" />
		<Engine name="Catalina" defaultHost="localhost" jvmRoute="node80">
			<Host name="localhost" appBase="webapp" unpackWARs="false"
				autoDeploy="false" xmlValidation="false" xmlNamespaceAware="false">
				<Context path="/" docBase="" reloadable="false" />
			</Host>
		</Engine>
	</Service>
</Server>

